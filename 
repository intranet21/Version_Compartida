[32m+[m[32mfunction paintForTypeElement(item) {[m
[32m+[m[32m    switch (item.N_FK_TIPO_ELEMENTO) {[m
[32m+[m[32m        //Redirect[m
[32m+[m[32m        case 1:[m
[32m+[m[32m            return `<div class="card mt-1 border">[m
[32m+[m[32m                <div class="card-header hoverBox" type="button" onclick="window.open('${item.T_URL_MENU}');return false;">[m
[32m+[m[32m                    <p class="font-weight-bold mb-0 p-2 text-info">${item.T_VALOR_CONTENT}</p>[m
[32m+[m[32m                </div >[m
[32m+[m[32m            </div >`;[m
[32m+[m[32m        //Se muestra infografÌas[m
[32m+[m[32m        case 5:[m
[32m+[m[32m            return `<img src="${item.T_URL_MENU}" alt="${item.T_VALOR_CONTENT}" />`;[m
[32m+[m[32m        //Se muestran los videos[m
[32m+[m[32m        case 13:[m
[32m+[m[32m            return `<iframe width="100%" height="315" src="${item.T_URL_MENU}" frameborder="0"[m
[32m+[m[32m                    allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture"[m
[32m+[m[32m                    allowfullscreen=""></iframe>`;[m
[32m+[m[32m        case 10:[m
[32m+[m[32m            return `<object data="${item.T_URL_MENU}" width="100px" height="85px"></object>`;[m
[32m+[m[32m        //Texto o HTML[m
[32m+[m[32m        case 15:[m
[32m+[m[32m            let content = item.T_DSC_CONTENT;[m
[32m+[m[32m            content += item.T_LINK_SITE_EXTRA !== null ? `<div class="text-center mt-4">[m
[32m+[m[32m                            <a href="${item.T_LINK_SITE_EXTRA}" class="btn btn-info font-weight-bold" target="_blank"> Visitar sitio[m
[32m+[m[32m                                &nbsp;&nbsp;<i class="fa fa-play faa-horizontal animated" style="color:#1f3a5a"></i>[m
[32m+[m[32m                                </a>[m
[32m+[m[32m                        </div>` : "";[m
[32m+[m[32m            return content;[m
[32m+[m[32m    }[m
 }[m
