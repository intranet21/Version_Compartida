[32m+[m							[32mbreak;[m
[32m+[m						[32mcase (int)EnumTipoElemento.MODAL:[m
[32m+[m							[32mstrItem += String.Format(Res_Paint_HTML.PAINT_AREAS_OPTION_MODAL, subIt.T_VALOR_MENU);[m
[32m+[m							[32mbreak;[m
